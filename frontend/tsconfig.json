{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "strict": true,
      "jsx": "react-jsxdev",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "lib": ["DOM", "ESNext"],
      "moduleResolution": "node",
      "isolatedModules": true
    },
    "include": ["src/**/*.tsx", "src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  